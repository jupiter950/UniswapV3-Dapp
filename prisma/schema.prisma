generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model users {
  user_id  BigInt @id @default(autoincrement())
  email    String @unique @db.VarChar
  password String @db.VarChar
  address  String @db.VarChar
}
